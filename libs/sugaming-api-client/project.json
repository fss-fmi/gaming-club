{
  "name": "sugaming-api-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/sugaming-api-client/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/sugaming-api-client",
        "main": "libs/sugaming-api-client/src/index.ts",
        "tsConfig": "libs/sugaming-api-client/tsconfig.lib.json",
        "assets": ["libs/sugaming-api-client/*.md"]
      },
      "dependsOn": ["generate"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/sugaming-api-client/**/*.ts",
          "libs/sugaming-api-client/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/sugaming-api-client/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "start-server-and-test \"nx serve sugaming-api:serve\" \"3000/api-json\" \"yarn openapi generate -i http://localhost:3000/api-json -o ./libs/sugaming-api-client/src/client/src --useOptions\""
          }
        ]
      }
    }
  },
  "tags": []
}
