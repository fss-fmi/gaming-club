{
  "version": 2,
  "buildCommand": "yarn prisma generate",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "yarn prisma generate && yarn nx build sugaming-api --prod",
        "outputDirectory": "../../dist/apps/sugaming-api"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ]
}
